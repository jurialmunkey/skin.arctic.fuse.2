<?xml version="1.0" encoding="UTF-8"?>
<window>

    <controls>
        <include>Dialog_Dim_Overlay</include>
        <control type="group">
            <include>Animation_SlideIn_Dialog</include>
            <control type="group">
                <include>Dimension_DialogConfirm</include>
                <control type="group">
                    <include content="Dialog_Background_Blur">
                        <param name="diffuse">diffuse/dialog_w1120_h400.png</param>
                    </include>
                    <include content="Dialog_Head">
                        <param name="top">40</param>
                        <param name="left">40</param>
                    </include>
                    <control type="group">
                        <include content="Dialog_Overlay_Mode_Glass">
                            <param name="flipped">false</param>
                        </include>
                        <control type="image">
                            <top>120</top>
                            <height>1</height>
                            <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                            <left>40</left>
                            <right>40</right>
                        </control>
                        <control type="group">
                            <include content="Dialog_Group">
                                <param name="top">160</param>
                                <param name="bottom">160</param>
                            </include>
                            <control type="textbox" id="9">
                                <top>-10</top>
                                <bottom>40</bottom>
                                <font>font_main_plot</font>
                                <textcolor>dialog_fg_70</textcolor>
                            </control>
                            <control type="group">
                                <bottom>0</bottom>
                                <height>12</height>
                                <control type="image">
                                    <height>1</height>
                                    <centertop>50%</centertop>
                                    <visible>Control.IsVisible(20)</visible>
                                    <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                                </control>
                                <control type="progress" id="20">
                                    <description>Progressbar</description>
                                    <info>System.Progressbar</info>
                                    <texturebg colordiffuse="00ffffff" border="6">progress/progress-bg.png</texturebg>
                                </control>
                                <control type="progress">
                                    <description>Progressbar</description>
                                    <info>System.Progressbar</info>
                                    <texturebg colordiffuse="00ffffff" border="6">progress/progress-bg.png</texturebg>
                                    <lefttexture colordiffuse="00ffffff" border="6">progress/progress-left.png</lefttexture>
                                    <midtexture colordiffuse="$VAR[ColorGradient]" border="6" diffuse="progress/diffuse.png">progress/progress-end.png</midtexture>
                                    <righttexture colordiffuse="$VAR[ColorGradient]" border="6" diffuse="progress/diffuse-end.png">progress/progress-right.png</righttexture>
                                    <include content="Defs_Progress_Colorfade">
                                        <param name="id">20</param>
                                    </include>
                                </control>
                            </control>
                        </control>
                    </control>

                    <control type="grouplist">
                        <right>40</right>
                        <left>40</left>
                        <height>80</height>
                        <bottom>20</bottom>
                        <align>center</align>
                        <orientation>horizontal</orientation>
                        <itemgap>20</itemgap>
                        <control type="button" id="11">
                            <width>280</width>
                            <visible>Control.IsEnabled(11)</visible>
                            <label>107</label>
                            <include>defs_dialog_button</include>
                        </control>
                        <control type="button" id="10">
                            <width>280</width>
                            <visible>Control.IsEnabled(10)</visible>
                            <label>106</label>
                            <include>defs_dialog_button</include>
                        </control>
                        <control type="button" id="12">
                            <width>280</width>
                            <visible>Control.IsEnabled(12)</visible>
                            <include>defs_dialog_button</include>
                        </control>
                    </control>
                </control>
            </control>
        </control>
    </controls>
</window>
